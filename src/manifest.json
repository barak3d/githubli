{
    "manifest_version": 2,
    "name": "githubli",
    "description": "Makes github code reviews less irritating by easing the navigation between files for code review using the keyboard arrows (UP/Down) and space for selection",
    "version": "1.7",
    "permissions": [       
		"*://*.github.com/*"
    ],
    "icons": {
        "16": "githubli-icon-16.png",
        "48": "githubli-icon-48.png",
        "128": "githubli-icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.github.com/*"],
            "css": [],
            "js": ["main.js"],
            "all_frames": true
        }
    ]
}